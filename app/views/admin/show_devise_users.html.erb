<h3>Зарегистрированные пользователи</h3>
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Email</th>
      <th>Player_Id</th>
      <th>Создан</th>
      <th>Изменен</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.player_id %></td>      
      <td><%= user.created_at %></td>
      <td><%= user.updated_at %></td>
    </tr>
  </tbody>
  <% end %>
</table>